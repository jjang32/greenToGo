{
  "managementHttpPort": null,
  "workers": {
    "cache": {
      "limit": 20
    },
    "concurrent": 4,
    "heartbeat": {
      "interval": 5,
      "timeout": 10
    },
    "lowPriorityThreshold": 0.8
  },
  "backendless": {
    "msgBroker": {
      "host": "cl.backendless.com",
      "port": 6379,
      "max_attempts": 5
    }
  },
  "app": {
    "id": "",
    "apiKey": "",
    "exclude": [
      "+(git|.idea|lib|test|docs|bin|deploy)/**",
      "+(LICENSE|*.md|*.json|.*)",
      "node_modules/**",
      "examples/services/wsdl-proxy/**"
    ]
  },
  "loggers": {
    "file": false,
    "workers": true
  },
  "sandbox": true,
  "verbose": false
}
